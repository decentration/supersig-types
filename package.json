{
  "name": "supersig-types",
  "version": "0.1.1",
  "description": "custom types for supersig node",
  "main": "index.js",
  "repository": "https://github.com/decentration/supersig-types",
  "author": "Ramsey Ajram <ramsey@decentration.org>",
  "license": "The Unlicense",
  "private": false,
  "scripts": {
    "build": "yarn generate:defs && yarn generate:meta",
    "generate:defs": "ts-node --skip-project node_modules/.bin/polkadot-types-from-defs --package supersig/interfaces --input ./src/interfaces --endpoint ./supersig-node.json",
    "generate:meta": "ts-node --skip-project node_modules/.bin/polkadot-types-from-chain --package supersig/interfaces --endpoint ./supersig-node.json --output ./src/interfaces --strict",
    "lint": "tsc --noEmit --pretty"
  },
  "dependencies": {
    "@polkadot/api": "^9.10.2"
  },
  "devDependencies": {
    "@polkadot/typegen": "^9.10.2",
    "ts-node": "^8.6.2",
    "typescript": "^4.6.2"
  }
}
